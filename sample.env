# Your server IP addres (curl -s ifcfg.me)
P2P_IP=<your server ip>

# Sequencer latest version
SEQUENCER_VERSION=0.85.0-alpha-testnet.5

# Erigon latest version
ERIGON_VERSION=v3.0.2
